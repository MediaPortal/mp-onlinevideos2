using MpeCore;
using MpeCore.Classes;
using System;
using System.IO;
using System.Windows.Forms;
using System.Xml;

public class Script
{
    public static void Main(PackageClass packageClass, UnInstallItem item)
    {
       if (!packageClass.Silent &amp;&amp; MessageBox.Show("Do you want to delete settings (complete OnlineVideos cleanup) ?", "Uninstall", MessageBoxButtons.YesNo) == DialogResult.Yes)
        {
            string CFG_SECTION = "onlinevideos";
            string CFG_THUMBNAIL_DIR = "thumbDir";

            string mePoConfigFile = Path.Combine(MpeInstaller.TransformInRealPath("%Config%"), "MediaPortal.xml");
            string ThumbsDir = Path.Combine(MpeInstaller.TransformInRealPath("%Thumbs%"), @"OnlineVideos\");

            XmlDocument configDoc = new XmlDocument();
            configDoc.Load(mePoConfigFile);
            XmlNode ovsConfigNode = configDoc.SelectSingleNode("//section[@name=\"" + CFG_SECTION + "\"]");
            if (ovsConfigNode != null)
            {
                XmlNode thumbsDirEntryNode = ovsConfigNode.SelectSingleNode("entry[@name=\"" + CFG_THUMBNAIL_DIR + "\"]");
                if (thumbsDirEntryNode != null) ThumbsDir = thumbsDirEntryNode.InnerText;

                try
                {
                    ovsConfigNode.ParentNode.RemoveChild(ovsConfigNode);
                    configDoc.Save(mePoConfigFile);
                }
                catch { }
            }

            ThumbsDir = ThumbsDir.Replace("/", @"\");
            if (Directory.Exists(ThumbsDir)) try { Directory.Delete(ThumbsDir, true); } catch { }

            string filename = Path.Combine(MpeInstaller.TransformInRealPath("%Config%"), "OnlineVideoSites.xml");
            if (File.Exists(filename)) try { File.Delete(filename); } catch { }

            string satelliteDllsPath = Path.Combine(MpeInstaller.TransformInRealPath("%Plugins%"), "Windows\\OnlineVideos");
            if (Directory.Exists(satelliteDllsPath)) try { Directory.Delete(satelliteDllsPath, true); } catch { }
        }
    }
}

